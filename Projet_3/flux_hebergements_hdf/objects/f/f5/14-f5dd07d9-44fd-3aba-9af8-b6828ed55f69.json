  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/44b37b4e-a1b6-398b-a330-25b8d6202410",
      "dc:description": {
        "de": [
          "Eine bewaldete, ruhige und erholsame Umgebung in einem Tal am Fuße der Artois-Hügel mit einem herrlichen Blick auf die Wälder."
        ],
        "en": [
          "A wooded, quiet and restful setting in a valley at the foot of the Artois hills, with a magnificent view of the woods."
        ],
        "it": [
          "Un ambiente boschivo tranquillo e rilassante in una valle ai piedi delle colline dell'Artois, con una magnifica vista sui boschi."
        ],
        "fr": [
          "Un cadre boisé, calme et reposant situé dans une vallée, au pied des collines d'Artois, avec une vue magnifique sur les bois."
        ],
        "es": [
          "Un entorno boscoso tranquilo y relajante en un valle al pie de las colinas de Artois, con una magnífica vista del bosque."
        ],
        "nl": [
          "Een rustige en ontspannende bosrijke omgeving in een vallei aan de voet van de Artois heuvels, met een prachtig uitzicht op de bossen."
        ]
      },
