      "dc:description": {
        "de": [
          "Wohnung Typ 2 mit 1 Schlafzimmer (1 Doppelbett), 1 Wohnzimmer mit Schlafcouch für 2 Personen, 1 Badezimmer, 1 WC, 1 Küchenecke mit Essbereich. Die Wohnung liegt im 1. Stock ohne Aufzug."
        ],
        "en": [
          "Apartment type 2 with 1 bedroom (1 double bed), 1 living room with sofa bed and double bed, 1 bathroom, 1 toilet, 1 kitchenette with dining area. Located on the 1st floor without elevator."
        ],
        "it": [
          "Appartamento tipo 2 con 1 camera da letto (1 letto matrimoniale), 1 soggiorno con divano letto e letto matrimoniale, 1 bagno con doccia, 1 WC, 1 angolo cottura con zona pranzo. Situato al 1° piano senza ascensore."
        ],
        "fr": [
          "Appartement type 2 avec 1 chambre (1 lit 2 places), 1 salon avec banquette convertible an lit 2 places, 1 salle d'eau, 1 WC, 1 coin cuisine avec espace repas. Situé au 1er étage sans ascenseur."
        ],
        "es": [
          "Piso tipo 2 con 1 dormitorio (1 cama doble), 1 sala de estar con un sofá cama y una cama doble, 1 cuarto de ducha, 1 aseo, 1 cocina americana con zona de comedor. Situado en la 1ª planta sin ascensor."
        ],
        "nl": [
          "Flat type 2 met 1 slaapkamer (1 tweepersoonsbed), 1 woonkamer met een slaapbank en een tweepersoonsbed, 1 doucheruimte, 1 toilet, 1 kitchenette met eethoek. Gelegen op de 1e verdieping zonder lift."
        ]
      },
