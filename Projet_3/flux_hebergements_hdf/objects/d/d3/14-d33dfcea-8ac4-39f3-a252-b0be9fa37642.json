      "dc:description": {
        "de": [
          "Residenz AQUARELLA, Wohnung im 3. Stock (kein Aufzug) mit Balkon gegenüber dem Meer, Wohnzimmer mit Clic-Clac, ausgestattete Küche, ein Schlafzimmer mit 2 Doppelstockbetten."
        ],
        "en": [
          "Residence AQUARELLA, Apartment on the 3rd floor (no elevator) with balcony facing the sea, living room with sofa bed, equipped kitchen, a bedroom with 2 double bunk beds."
        ],
        "it": [
          "Residence AQUARELLA, appartamento al 3° piano (senza ascensore) con balcone fronte mare, soggiorno con divano letto, cucina attrezzata, camera con 2 letti a castello matrimoniali."
        ],
        "fr": [
          "Résidence AQUARELLA, Appartement au 3ème étage (pas d'ascenseur) avec balcon face mer, séjour avec clic-clac, cuisine équipée, une chambre avec 2 lits doubles superposés."
        ],
        "es": [
          "Residencia AQUARELLA, Apartamento en la 3ª planta (sin ascensor) con balcón frente al mar, salón con sofá cama, cocina equipada, dormitorio con 2 literas dobles."
        ],
        "nl": [
          "Residence AQUARELLA, Appartement op de 3e verdieping (geen lift) met balkon aan zee, woonkamer met slaapbank, ingerichte keuken, slaapkamer met 2 tweepersoons stapelbedden."
        ]
      },
