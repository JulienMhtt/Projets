      "dc:description": {
        "de": [
          "Camping le Paradis, 300m von allen Geschäften und 3km vom Strand von Berck entfernt. Kostenloser Angelteich - kostenloser WLAN-Zugang - Kinderspielplatz, Bouleplatz,"
        ],
        "en": [
          "Campsite Le Paradis 300m from all shops 3km from the beach of Berck. Free fishing pond - free wifi - children's playground, boules pitch,"
        ],
        "it": [
          "Camping le Paradis a 300 m da tutti i negozi e a 3 km dalla spiaggia di Berck. Laghetto di pesca gratuito - wifi gratuito - parco giochi per bambini, campo da bocce,"
        ],
        "fr": [
          "Camping le Paradis à 300m de tous commerces à 3km de la plage de Berck. Etang de pêche gratuit - wifi gratuit - parc de jeux pour enfant, terrain de boules,"
        ],
        "es": [
          "Camping le Paradis a 300m de todos los comercios y a 3km de la playa de Berck. Estanque de pesca gratuito - wifi gratuito - parque infantil, bolera,"
        ],
        "nl": [
          "Camping le Paradis 300m van alle winkels en 3km van het strand van Berck. Gratis visvijver - gratis wifi - kinderspeelplaats, jeu de boules baan,"
        ]
      },
