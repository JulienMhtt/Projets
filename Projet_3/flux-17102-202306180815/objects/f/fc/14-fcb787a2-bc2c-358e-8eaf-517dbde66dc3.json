      "@id": "https://data.datatourisme.fr/4e0fad55-a8c7-3e82-94ca-77ce57093449",
      "dc:description": {
        "de": [
          "DIE WOHNUNG BEFINDET SICH IN EINER RUHIGEN WOHNGEGEND MIT BLICK AUF NOTRE DAME. CUISINE AMENENAGE ET EQUIPE. RENOVIERTE UND LIEBEVOLL DEKORIERTE WOHNUNG"
        ],
        "en": [
          "LUXURY APARTMENT WITH VIEW ON NOTRE DAME IN THE DISTRICT OF THE LAST UNDER. KITCHEN FURNISHED AND EQUIPPED. APARTMENT RENOVATED AND DECORATED WITH CARE"
        ],
        "it": [
          "APPARTAMENTO DI LUSSO CON VISTA SU NOTRE DAME NEL QUARTIERE DELL'ULTIMO CENTESIMO. CUCINA ARREDATA E ATTREZZATA. APPARTAMENTO RISTRUTTURATO E DECORATO CON CURA"
        ],
        "fr": [
          "APPARTEMENT DE STANDING AVEC VUE SUR NOTRE DAME DANS LE QUARTIER DU DERNIER SOUS. CUISINE AMENENAGE ET EQUIPE. APPARTEMENT RENOVEE ET DECOREE AVEC SOIN"
        ],
        "es": [
          "PISO DE LUJO CON VISTAS A NOTRE DAME EN EL BARRIO DEL ÚLTIMO CENTAVO. COCINA AMUEBLADA Y EQUIPADA. PISO RENOVADO Y DECORADO CON ESMERO"
        ],
        "nl": [
          "LUXE APPARTEMENT MET UITZICHT OP NOTRE DAME IN DE WIJK VAN DE LAATSTE CENT. KEUKEN INGERICHT EN UITGERUST. MET ZORG GERENOVEERDE EN INGERICHTE FLAT"
        ]
      },
