      "dc:description": {
        "de": [
          "150m vom Strand entfernt, Einkaufsmöglichkeiten, Kino in der Nähe. Studio im ersten Stock eines Hauses. Kochnische, Badezimmer, elektrische Heizung, Gemeinschaftsgarten. TV"
        ],
        "en": [
          "At 150m from the beach, shops, cinema nearby. Studio on the 1st floor of a house. Kitchenette, bathroom, electric heating, shared garden. TV"
        ],
        "it": [
          "A 150 m dalla spiaggia, negozi, cinema nelle vicinanze. Studio al 1° piano di una casa. Angolo cottura, bagno, riscaldamento elettrico, giardino in comune. TV"
        ],
        "fr": [
          "A 150m de la plage, commerces, cinéma à proximité. Studio au 1er étage d'une maison. Kitchenette, salle de bain, chauffage électrique, jardin commun. TV"
        ],
        "es": [
          "A 150m de la playa, tiendas, cine en las cercanías. Estudio en el primer piso de una casa. Cocina americana, baño, calefacción eléctrica, jardín compartido. TV"
        ],
        "nl": [
          "150m van het strand, winkels, bioscoop in de buurt. Studio op de 1e verdieping van een huis. Kitchenette, badkamer, elektrische verwarming, gemeenschappelijke tuin. TV"
        ]
      },
