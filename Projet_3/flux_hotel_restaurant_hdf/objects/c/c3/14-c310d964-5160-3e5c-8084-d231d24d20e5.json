  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/95e3ad99-8fa7-378d-a232-b113c2057d90",
      "dc:description": {
        "de": [
          "Im Kiefernwald eingebettete Cottages, komplett ausgestattet, Holzterrasse, großer Raum, ruhig und mit Bäumen bepflanzt. Haustiere auf Anfrage erlaubt."
        ],
        "en": [
          "Cottages nestled in the pine forest, fully equipped, wooden terrace, large space, quiet and wooded. Animals allowed on request."
        ],
        "it": [
          "Cottage immersi nella pineta, completamente attrezzati, terrazza in legno, ampi spazi, tranquillità e alberi. Animali ammessi su richiesta."
        ],
        "fr": [
          "Cottages nichés dans la pinède, tout équipés, terrasse en bois, grand espace, calme et arboré. Animaux autorisés sur demande."
        ],
        "nl": [
          "Huisjes genesteld in het dennenbos, volledig uitgerust, houten terras, grote ruimte, rust en bomen. Huisdieren toegestaan op aanvraag."
        ],
        "es": [
          "Cabañas enclavadas en el bosque de pinos, totalmente equipadas, terraza de madera, gran espacio, tranquilidad y árboles. Se admiten mascotas bajo petición."
        ]
      },
